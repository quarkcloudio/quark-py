    "#coding: utf-8\n",
    "from quark import Quark\n",
    "\n",
    "# 创建一个app\n",
    "app = Quark(__name__)\n",
    "\n",
    "# 设置debug模式\n",
    "app.config[\"DEBUG\"] = False\n",
    "\n",
    "# 创建路由\n",
    "@app.route('/')\n",
    "def index() -> str:\n",
    "    return 'Hello World!'\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    app.run(host=\"0.0.0.0\", port=3000)\n"
   ]
